@using WebHarness.Ui.Models
@model StudentRosterViewModel

@{
    ViewBag.Title = "Student Roster";

}

<h2>Studen Roster</h2>

<table>
    <thead>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var student in Model.Students)
    {
        <tr><td>@student.FirstName</td><td>@student.LastName</td></tr>
    }
    </tbody>
</table>

